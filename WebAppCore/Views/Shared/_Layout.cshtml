    <script src="~/lib/mdb/js/jquery-3.4.0.min.js"></script>
    <script src="~/lib/mdb/js/popper.min.js"></script>
    <script src="~/lib/mdb/js/bootstrap.js"></script>
    <script src="~/lib/mdb/js/mdb.js"></script>
    <script src="~/lib/mdb/js/site.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
